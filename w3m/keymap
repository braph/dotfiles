keymap	+	NEXT_PAGE
keymap	,	LEFT
keymap	-	PREV_PAGE
keymap	.	RIGHT
#keymap	:	MARK_URL
keymap	";"	MARK_WORD
keymap	=	INFO
keymap	D	DOWNLOAD_LIST
