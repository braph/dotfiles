# braph's version of ~/.w3m/keymap

# Ctrl  : C-, ^
# Escape: ESC-, M-, ^[
# Space : SPC, ' '
# Tab   : TAB, ^i, ^I
# Delete: DEL, ^?
# Up    : UP, ^[[A
# Down  : DOWN, ^[[B
# Right : RIGHT, ^[[C
# Left  : LEFT, ^[[D

# std movement
keymap   h  MOVE_LEFT
keymap   j  MOVE_DOWN
keymap   k  MOVE_UP
keymap   l  MOVE_RIGHT

keymap   UP    MOVE_UP
keymap   DOWN  MOVE_DOWN
keymap   RIGHT MOVE_RIGHT
keymap   LEFT  MOVE_LEFT

keymap   u  UNDO
keymap   r  REDO

keymap   w  NEXT_WORD
keymap   b  PREV_WORD

keymap   0  LINE_BEGIN
keymap   $  LINE_END

keymap   C-u   PREV_PAGE
keymap   C-d   NEXT_PAGE

keymap   J  NEXT_PAGE
keymap   K  PREV_PAGE

#keymap   :  COMMAND
keymap   :  GOTO_LINE

#keymap	C-u	PREV_LINK

keymap   g  BEGIN
keymap   G  END
keymap	z	CENTER_V

keymap   n  SEARCH_NEXT
keymap   N  SEARCH_PREV
keymap   /  SEARCH
keymap   ?  SEARCH_BACK

keymap   C-l   REDRAW
keymap   C-g   LINE_INFO

#keymap   Q  EXIT
keymap	C-h   HELP
keymap   Q  QUIT
#

# browser stuff
keymap   O  LOAD
keymap   o  GOTO
keymap   C-r  RELOAD
keymap   R  PEEK

keymap   t     TAB_LINK
keymap   C-t   PEEK_LINK
keymap   T     NEW_TAB

keymap   }  NEXT_TAB
keymap   {  PREV_TAB
keymap   x  CLOSE_TAB

keymap   B  BACK
keymap	d	SAVE_LINK

keymap	<	SHIFT_LEFT
keymap	>	SHIFT_RIGHT

keymap	ESC-o OPTIONS
keymap   ESC-s SAVE_SCREEN

keymap   i  VIEW_IMAGE
keymap   I  PEEK_IMG
keymap	L	LIST
keymap	H  HISTORY

keymap   f  LIST_MENU
#

keymap	M	EXTERN
