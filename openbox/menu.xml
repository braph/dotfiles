  <separator label="Music" />

  <item label="Play" icon="/usr/share/icons/gnome/16x16/actions/player_play.png">
     <action name="Execute"> <execute> mpc -h workstation play </execute> </action>
  </item>

  <item label="Pause" icon="/usr/share/icons/gnome/16x16/actions/player_pause.png">
     <action name="Execute"> <execute> mpc -h workstation pause </execute> </action>
  </item>

